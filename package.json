{
  "name": "wosis",
  "packageManager": "yarn@4.3.1",
  "version": "1.0.0",
  "description": "iso25258 ISRIC implementation",
  "repository": {
    "type": "git",
    "url": "ssh://git@git.wur.nl:isric/databases/wosis.git"
  },
  "license": "UNLICENSED",
  "author": "Luis Calisto <luis.calisto@isric.org>",
  "scripts": {
    "gm": "yarn run graphile-migrate",
    "make-release": "sh -c 'pg_dump iso28258 --inserts --no-owner --no-security-labels -n core -n metadata | sed -e \"/^-- Dumped by pg_dump/d\" -e \"/^..*restrict/d\" > releases/$1' --"
  },
  "dependencies": {
    "graphile-migrate": "^1.3.0"
  }
}
